{
  "name": "@vigilio/express",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "create web apps with @vigilio/express now",
  "scripts": {
    "dev": "vite",
    "serve": "bun --watch app/main.ts ",
    "build:serve": "rimraf dist && tsc  --project tsconfig.build.json ",
    "build:dev": "tsc --project ./resources/tsconfig.json && bun dev build ",
    "start": "NODE_ENV=production bun dist/main.js",
    "test": "vitest",
    "biome:format:server": " biome format  ./app --write",
    "biome:check:server": "biome check ./app",
    "biome:format:client": "biome format  ./resources/ts --write",
    "biome:check:client": "biome check ./resources/ts",
    "biome:format": "bun biome:format:server && bun biome:format:client",
    "biome:check": "bun biome:check:server && bun biome:check:client"
  },
  "keywords": [
    "@vigilio/express",
    "@vigilio/express-api",
    "bun"
  ],
  "author": "Vigilio",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hookform/resolvers": "3.4.2",
    "@preact/signals": "^1.3.0",
    "@vigilio/express-core": "^1.9.10",
    "@vigilio/preact-fetching": "^2.0.14",
    "@vigilio/preact-paginator": "^0.0.10",
    "@vigilio/preact-table": "^1.0.19",
    "@vigilio/sweet": "^1.1.2",
    "@vigilio/valibot": "^0.0.8",
    "alpinejs": "^3.14.1",
    "aos": "^2.3.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "formidable": "^3.5.1",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "memorystore": "^1.6.7",
    "million": "^2.6.4",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "preact": "^10.22.1",
    "quill": "^2.0.2",
    "quill-image-resize": "^3.0.9",
    "react-hook-form": "^7.52.1",
    "react-leaflet": "^4.2.1",
    "react-slick": "^0.30.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "sharp": "0.33.4",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.6.3",
    "valibot": "^0.20.1",
    "wouter-preact": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@preact/preset-vite": "^2.8.3",
    "@types/alpinejs": "^3.13.10",
    "@types/aos": "^3.0.7",
    "@types/bun": "^1.1.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/formidable": "^3.4.5",
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.3",
    "@types/react-slick": "^0.23.13",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "rimraf": "^5.0.8",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.3.3",
    "vite-plugin-live-reload": "^3.0.3",
    "vitest": "^1.6.0"
  }
}
